{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write .",
      "git add ."
    ]
  },
  "scripts": {
    "test": "vitest",
    "build:": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "slugify": "^1.6.5",
    "typeorm": "^0.3.10",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "supertest": "^6.3.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "vitest": "^0.24.3",
    "vitest-tsconfig-paths": "^3.4.1"
  }
}
